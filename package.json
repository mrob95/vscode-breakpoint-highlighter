{
  "name": "breakpoint-highlighter",
  "displayName": "breakpoint-highlighter",
  "publisher": "mrob95",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrob95/vscode-breakpoint-highlighter"
  },
  "bugs": {
    "url": "https://github.com/mrob95/vscode-breakpoint-highlighter/issues"
  },
  "description": "",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Breakpoint Highlighter",
      "properties": {
        "breakpointHighlighter.backgroundColor": {
          "type": "string",
          "default": "rgba(255, 0, 0, 0.2)",
          "description": "Background color"
        },
        "breakpointHighlighter.afterColor": {
          "type": "string",
          "default": "#FF0000",
          "description": "After color"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
